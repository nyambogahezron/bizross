{
	"$schema": "https://turborepo.com/schema.json",
	"pipeline": {
		"build": {
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"dependsOn": ["^build"],
			"outputs": [
				"build/**",
				".vercel/**",
				"dist/**",
				".next/**",
				"!.next/cache/**"
			]
		},
		"start": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		},
		"test": {
			"outputs": ["coverage/**"],
			"dependsOn": []
		},
		"lint": {
			"dependsOn": ["^build", "^lint"]
		},
		"lint:fix": {
			"dependsOn": ["^build"]
		},
		"format": {
			"dependsOn": []
		},
		"check": {
			"dependsOn": []
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"clean": {
			"dependsOn": []
		}
	}
}
